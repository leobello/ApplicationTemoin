<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox alignment="CENTER_LEFT" maxHeight="40.0" minHeight="40.0" prefHeight="40.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="photo" fitHeight="40.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="nomPrenom" text="Nom Prenom" />
      <Region fx:id="region" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
      <Button fx:id="ajouter" mnemonicParsing="false" prefHeight="25.0" prefWidth="123.0" text="Ajouter" />
   </children>
   <padding>
      <Insets bottom="2.0" left="16.0" right="16.0" top="2.0" />
   </padding>
</HBox>
